        <value>(function (inputs, outputs) {&#13;
  var raw = (inputs.email || '').trim();&#13;
  if (!raw) { outputs.domain = ''; return; }&#13;
  // Support "Name &lt;name@example.com&gt;" and mailto:&#13;
  var addr = (/&lt;([^&gt;]+)&gt;/.exec(raw) || [])[1] || raw.replace(/^mailto:/i, '').trim();&#13;
  var m = /^[^@\s]+@([A-Za-z0-9.-]+\.[A-Za-z]{2,})$/.exec(addr);&#13;
  outputs.domain = m ? m[1].toLowerCase() : '';&#13;
})(inputs, outputs);</value>
