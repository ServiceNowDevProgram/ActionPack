        <value>(function execute(inputs, outputs) {&#13;
// ... code ...&#13;
var usr=inputs.tocheckmng;&#13;
var arr=[]; //empty array to capture all the reportees&#13;
var gr= new GlideRecord("sys_user");&#13;
gr.addQuery("manager",usr);&#13;
gr.query();&#13;
while(gr.next()){&#13;
arr.push(gr.sys_id);&#13;
}&#13;
&#13;
outputs.usrid=arr.join(",");&#13;
})(inputs, outputs);&#13;
</value>
